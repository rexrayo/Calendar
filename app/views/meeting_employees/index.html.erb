<p id="notice"><%= notice %></p>

<h1>Meeting Employees</h1>

<table>
  <thead>
    <tr>
      <th>Meeting</th>
      <th>Employee</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @meeting_employees.each do |meeting_employee| %>
      <tr>
        <td><%= meeting_employee.meeting %></td>
        <td><%= meeting_employee.employee %></td>
        <td><%= link_to 'Show', meeting_employee %></td>
        <td><%= link_to 'Edit', edit_meeting_employee_path(meeting_employee) %></td>
        <td><%= link_to 'Destroy', meeting_employee, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Meeting Employee', new_meeting_employee_path %>
